{
  "name": "web-components-cli",
  "description": "Web Component generator (based on Angular 7 elements)",
  "version": "0.0.0",
  "author": "Mariano Moreno",
  "bin": {
    "webcomp": "./bin/run"
  },
  "bugs": "https://github.com/hedesil/web-components-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.12",
    "@oclif/config": "^1.12.12",
    "@oclif/plugin-help": "^2.1.6",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@types/node": "^10.14.4",
    "globby": "^8.0.2",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/hedesil/web-components-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "webcomp",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "hedesil/web-components-cli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
